#include<iostream>
using namespace std;
int main(){
    int n, res=0;
    cin>>n;
    while(n--){
        bool a, b, c;
        int count=0;
        cin>>a>>b>>c;
        if(a==1){
            count++;
        }
        if(b==1){
            count++;
        }
        if(c==1){
            count++;
        }
        if(count>=2){
            res++;
        }
    }
    cout<<res<<endl;
    return 0;
    
}
